<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="../javascripts/search.js" type="text/javascript"></script>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>


    <form class="myForm" action="/search" method="get">
      <input class="inp" type="text" autocomplete="off" autofocus name="query" placeholder="Type Here"/>
      <input type="submit" value="save"/>
    </form>


    <ul id="objs">
    </ul>

    <script type="text/javascript">




      var data = <%- JSON.stringify(data) %> ;

      var obj = document.getElementById('objs');

      for (var i=0; i< data.length; i++){
        var li = document.createElement("li");
        var link = data[i][1];//JSON.stringify(data[i][1]);
        var a = document.createElement('a');
        a.textContent = data[i][0];
        a.setAttribute('href', link);
        li.appendChild(a);
        obj.appendChild(li);
      }





    </script>

  </body>
</html>
